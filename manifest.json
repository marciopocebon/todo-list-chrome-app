{
  "manifest_version": 2,
  "name": "Todo list",
  "description": "Simple todo list to add and remove items easily",
  "version": "0.1",
  "permissions": [
  	"storage"
  ],
  "app": {
    "background": {
      "scripts": ["js/main.js"]
    }
  },
  "icons": {
    "16": "img/todo-list-16.jpg",
    "128": "img/todo-list-128.jpg"
  },
  "minimum_chrome_version": "23"
}